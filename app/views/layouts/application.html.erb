<!DOCTYPE html>
<html>
<head>
  <title>CsTemplate</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'shared/message' %>
  <nav class="navbar navbar-fixed-top navbar-dark bg-inverse">
    <a class="navbar-brand" href="<%= root_path %>">Trung Manucian Ticket</a>
    <ul class="nav navbar-nav pull-right">

      <% if user_signed_in? %>

           <li class="nav-item">
              <%= link_to "Create event",new_event_path,class: "nav-link"  %>
          </li>
          <li class="nav-item">
             <%= link_to "Your events",user_events_path,class: "nav-link"  %>
          </li>
           <li class="nav-item">
             <%= link_to "Your Tickets",user_tickets_path,class: "nav-link"  %>
          </li>
          <li class="nav-item">
             <%= link_to "Log out",destroy_user_session_path,class: "nav-link",method: 'delete'  %>
          </li>
      <% else %>
      <li class="nav-item">
        <%= link_to "Log In",new_user_session_path,class: "nav-link"  %>
      </li>
      <li class="nav-item">
        <%= link_to "Sign up",new_user_registration_path,class: "nav-link"  %>
      </li>
      <% end %>


    </ul>
  </nav>

  <!-- <div class="container"> -->
    <%= yield %>
  <!-- </div> -->

</body>
</html>
